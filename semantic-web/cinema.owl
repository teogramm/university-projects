@prefix : <http://teochris.local/cinema#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix spin: <http://spinrdf.org/spin#> .
@prefix cinema: <http://teochris.local/cinema#> .
@base <http://teochris.local/cinema> .

<http://teochris.local/cinema> rdf:type owl:Ontology ;
                                spin:imports <http://topbraid.org/spin/rdfsplus> ;
                                owl:versionInfo "Created with TopBraid Composer" .

#################################################################
#    Annotation properties
#################################################################

###  http://spinrdf.org/spin#imports
spin:imports rdf:type owl:AnnotationProperty .


###  http://teochris.local/cinema#movieName
cinema:movieName rdf:type owl:AnnotationProperty ;
                 rdfs:subPropertyOf cinema:name ;
                 rdfs:range xsd:string ;
                 rdfs:domain cinema:Movie .


###  http://teochris.local/cinema#name
cinema:name rdf:type owl:AnnotationProperty ;
            rdfs:label "name" ;
            rdfs:subPropertyOf rdfs:label ;
            rdfs:range xsd:string .


###  http://teochris.local/cinema#seriesName
cinema:seriesName rdf:type owl:AnnotationProperty ;
                  rdfs:comment "Name of a movie series" ;
                  rdfs:label "series name" ;
                  rdfs:subPropertyOf cinema:name ;
                  rdfs:range xsd:string ;
                  rdfs:domain cinema:MovieSeries .


#################################################################
#    Datatypes
#################################################################

###  http://www.w3.org/2001/XMLSchema#date
xsd:date rdf:type rdfs:Datatype .


#################################################################
#    Object Properties
#################################################################

###  http://teochris.local/cinema#actedIn
cinema:actedIn rdf:type owl:ObjectProperty ;
               rdfs:subPropertyOf cinema:contributedTo ;
               owl:inverseOf cinema:hasActor ;
               rdfs:domain cinema:Actor ;
               rdfs:range cinema:Movie ;
               rdfs:label "acted in" .


###  http://teochris.local/cinema#belongsToGenre
cinema:belongsToGenre rdf:type owl:ObjectProperty ;
                      rdfs:domain cinema:Movie ;
                      rdfs:range cinema:Genre ;
                      rdfs:label "Belongs to Genre" .


###  http://teochris.local/cinema#contributedTo
cinema:contributedTo rdf:type owl:ObjectProperty ;
                     owl:inverseOf cinema:hasContributor ;
                     rdfs:domain cinema:Person ;
                     rdfs:range cinema:Movie ;
                     rdfs:label "contributed" .


###  http://teochris.local/cinema#directed
cinema:directed rdf:type owl:ObjectProperty ;
                rdfs:subPropertyOf cinema:contributedTo ;
                owl:inverseOf cinema:directedBy ;
                rdfs:domain cinema:Director ;
                rdfs:range cinema:Movie ;
                rdfs:label "directed" .


###  http://teochris.local/cinema#directedBy
cinema:directedBy rdf:type owl:ObjectProperty ;
                  rdfs:subPropertyOf cinema:hasContributor .


###  http://teochris.local/cinema#givesOut
cinema:givesOut rdf:type owl:ObjectProperty ;
                owl:inverseOf cinema:isGivenBy ;
                rdfs:domain cinema:AwardsOrganization ;
                rdfs:range cinema:Award ;
                rdfs:comment "States that an organization gives out an award" ;
                rdfs:label "gives out" .


###  http://teochris.local/cinema#hasActor
cinema:hasActor rdf:type owl:ObjectProperty ;
                rdfs:subPropertyOf cinema:hasContributor .


###  http://teochris.local/cinema#hasAward
cinema:hasAward rdf:type owl:ObjectProperty ;
                rdfs:range cinema:Award ;
                rdfs:label "has award" .


###  http://teochris.local/cinema#hasContributor
cinema:hasContributor rdf:type owl:ObjectProperty .


###  http://teochris.local/cinema#hasMovie
cinema:hasMovie rdf:type owl:ObjectProperty ;
                owl:inverseOf cinema:partOf ;
                rdf:type owl:InverseFunctionalProperty ;
                rdfs:label "Series has movie" .


###  http://teochris.local/cinema#hasPrequel
cinema:hasPrequel rdf:type owl:ObjectProperty ;
                  owl:inverseOf cinema:hasSequel ;
                  rdf:type owl:FunctionalProperty ,
                           owl:InverseFunctionalProperty .


###  http://teochris.local/cinema#hasProduced
cinema:hasProduced rdf:type owl:ObjectProperty ;
                   owl:inverseOf cinema:producedBy .


###  http://teochris.local/cinema#hasSequel
cinema:hasSequel rdf:type owl:ObjectProperty ;
                 rdfs:subPropertyOf owl:topObjectProperty ;
                 rdf:type owl:FunctionalProperty ,
                          owl:InverseFunctionalProperty ;
                 rdfs:domain cinema:Movie ;
                 rdfs:range cinema:Movie .


###  http://teochris.local/cinema#hasSubsidiary
cinema:hasSubsidiary rdf:type owl:ObjectProperty ,
                              owl:TransitiveProperty ;
                     rdfs:domain cinema:Company ;
                     rdfs:range cinema:Company ;
                     rdfs:label "Has subsidiary" .


###  http://teochris.local/cinema#isGivenBy
cinema:isGivenBy rdf:type owl:ObjectProperty ;
                 rdfs:label "Given by" .


###  http://teochris.local/cinema#movieHasAward
cinema:movieHasAward rdf:type owl:ObjectProperty ;
                     rdfs:subPropertyOf cinema:hasAward ;
                     rdfs:domain cinema:Movie ;
                     rdfs:range cinema:MovieAward ;
                     rdfs:label "Movie has award" .


###  http://teochris.local/cinema#partOf
cinema:partOf rdf:type owl:ObjectProperty ,
                       owl:FunctionalProperty ;
              rdfs:domain cinema:Movie ;
              rdfs:range cinema:MovieSeries ;
              rdfs:comment "States that a movie is part of a movie series" ;
              rdfs:label "Part of" .


###  http://teochris.local/cinema#personHasAward
cinema:personHasAward rdf:type owl:ObjectProperty ;
                      rdfs:subPropertyOf cinema:hasAward ;
                      rdfs:domain cinema:Person ;
                      rdfs:range cinema:PersonAward ;
                      rdfs:label "person has award" .


###  http://teochris.local/cinema#producedBy
cinema:producedBy rdf:type owl:ObjectProperty ;
                  rdfs:domain cinema:Movie ;
                  rdfs:range cinema:ProductionCompany ;
                  rdfs:label "Produced By" .


###  http://teochris.local/cinema#starredIn
cinema:starredIn rdf:type owl:ObjectProperty ;
                 rdfs:subPropertyOf cinema:actedIn ;
                 rdfs:domain cinema:Actor ;
                 rdfs:range cinema:Movie ;
                 rdfs:label "starred in" .


###  http://teochris.local/cinema#writtenBy
cinema:writtenBy rdf:type owl:ObjectProperty ;
                 rdfs:subPropertyOf cinema:hasContributor ;
                 owl:inverseOf cinema:wrote .


###  http://teochris.local/cinema#wrote
cinema:wrote rdf:type owl:ObjectProperty ;
             rdfs:subPropertyOf cinema:contributedTo ;
             rdfs:domain cinema:Writer ;
             rdfs:range cinema:Movie ;
             rdfs:label "wrote" .


#################################################################
#    Data properties
#################################################################

###  http://teochris.local/cinema#aboutGenre
cinema:aboutGenre rdf:type owl:DatatypeProperty ;
                  rdfs:domain cinema:Genre ;
                  rdfs:range xsd:string ;
                  rdfs:label "about genre" .


###  http://teochris.local/cinema#dateOfBirth
cinema:dateOfBirth rdf:type owl:DatatypeProperty ;
                   rdfs:domain cinema:Person ;
                   rdfs:range xsd:date ;
                   rdfs:label "date of birth" .


###  http://teochris.local/cinema#dateofDeath
cinema:dateofDeath rdf:type owl:DatatypeProperty ;
                   rdfs:domain cinema:Person ;
                   rdfs:range xsd:date ;
                   rdfs:label "dateof death" .


###  http://teochris.local/cinema#description
cinema:description rdf:type owl:DatatypeProperty ;
                   rdfs:range xsd:string ;
                   rdfs:comment "A short description of the movie" ;
                   rdfs:label "Description" .


###  http://teochris.local/cinema#duration
cinema:duration rdf:type owl:DatatypeProperty ;
                rdfs:domain cinema:Movie ;
                rdfs:range xsd:integer ;
                rdfs:comment "Movie duration in minutes" ;
                rdfs:label "duration" .


###  http://teochris.local/cinema#firstName
cinema:firstName rdf:type owl:DatatypeProperty ;
                 rdfs:domain cinema:Person ;
                 rdfs:range xsd:string ;
                 rdfs:label "first name" .


###  http://teochris.local/cinema#imdbId
cinema:imdbId rdf:type owl:DatatypeProperty ,
                       owl:FunctionalProperty ;
              rdfs:range xsd:string ;
              rdfs:comment "ID of the movie on imdb" .


###  http://teochris.local/cinema#isAlive
cinema:isAlive rdf:type owl:DatatypeProperty ;
               rdfs:domain cinema:Person ;
               rdfs:range xsd:boolean ;
               rdfs:label "is alive" .


###  http://teochris.local/cinema#lastName
cinema:lastName rdf:type owl:DatatypeProperty ;
                rdfs:domain cinema:Person ;
                rdfs:range xsd:string ;
                rdfs:label "last name" .


###  http://teochris.local/cinema#movieName
cinema:movieName rdf:type owl:DatatypeProperty .


###  http://teochris.local/cinema#releaseYear
cinema:releaseYear rdf:type owl:DatatypeProperty ;
                   rdfs:domain cinema:Movie ;
                   rdfs:range xsd:integer ;
                   rdfs:comment "Year the movie was released." ;
                   rdfs:label "release year" .


#################################################################
#    Classes
#################################################################

###  http://teochris.local/cinema#Actor
cinema:Actor rdf:type owl:Class ;
             rdfs:subClassOf cinema:Person ;
             rdfs:label "Actor" .


###  http://teochris.local/cinema#AnimatedMovie
cinema:AnimatedMovie rdf:type owl:Class ;
                     rdfs:subClassOf cinema:Movie ;
                     rdfs:comment "Ταινία κινουμένων σχεδίων" .


###  http://teochris.local/cinema#AnimatedMovieProductionCompany
cinema:AnimatedMovieProductionCompany rdf:type owl:Class ;
                                      rdfs:subClassOf cinema:ProductionCompany ,
                                                      [ rdf:type owl:Restriction ;
                                                        owl:onProperty cinema:hasProduced ;
                                                        owl:allValuesFrom cinema:AnimatedMovie
                                                      ] ;
                                      rdfs:comment "Εταιρεία παραγωγής που παράγει ταινίες κινουμένων σχεδίων" .


###  http://teochris.local/cinema#Award
cinema:Award rdf:type owl:Class ;
             rdfs:label "Award" .


###  http://teochris.local/cinema#AwardedActor
cinema:AwardedActor rdf:type owl:Class ;
                    owl:equivalentClass [ owl:intersectionOf ( cinema:Actor
                                                               cinema:AwardedPerson
                                                             ) ;
                                          rdf:type owl:Class
                                        ] ;
                    rdfs:subClassOf cinema:Actor .


###  http://teochris.local/cinema#AwardedDirector
cinema:AwardedDirector rdf:type owl:Class ;
                       owl:equivalentClass [ owl:intersectionOf ( cinema:AwardedPerson
                                                                  cinema:Director
                                                                ) ;
                                             rdf:type owl:Class
                                           ] ;
                       rdfs:subClassOf cinema:Director .


###  http://teochris.local/cinema#AwardedMovie
cinema:AwardedMovie rdf:type owl:Class ;
                    owl:equivalentClass [ rdf:type owl:Restriction ;
                                          owl:onProperty cinema:movieHasAward ;
                                          owl:someValuesFrom cinema:Award
                                        ] ;
                    rdfs:subClassOf cinema:Movie .


###  http://teochris.local/cinema#AwardedPerson
cinema:AwardedPerson rdf:type owl:Class ;
                     owl:equivalentClass [ rdf:type owl:Restriction ;
                                           owl:onProperty cinema:personHasAward ;
                                           owl:someValuesFrom cinema:Award
                                         ] ;
                     rdfs:subClassOf cinema:Person .


###  http://teochris.local/cinema#AwardedWriter
cinema:AwardedWriter rdf:type owl:Class ;
                     owl:equivalentClass [ owl:intersectionOf ( cinema:AwardedPerson
                                                                cinema:Writer
                                                              ) ;
                                           rdf:type owl:Class
                                         ] ;
                     rdfs:subClassOf cinema:Writer .


###  http://teochris.local/cinema#AwardsOrganization
cinema:AwardsOrganization rdf:type owl:Class ;
                          rdfs:comment "An organization that gives out awards related to motion pictures." ;
                          rdfs:label "Awards organization" .


###  http://teochris.local/cinema#BestPictureAwardedMovie
cinema:BestPictureAwardedMovie rdf:type owl:Class ;
                               owl:equivalentClass [ rdf:type owl:Restriction ;
                                                     owl:onProperty cinema:movieHasAward ;
                                                     owl:hasValue cinema:BestPictureOscar
                                                   ] ;
                               rdfs:subClassOf cinema:AwardedMovie ;
                               rdfs:comment "Movies that have been awarded the BestPicture Oscar" .


###  http://teochris.local/cinema#Company
cinema:Company rdf:type owl:Class .


###  http://teochris.local/cinema#Director
cinema:Director rdf:type owl:Class ;
                rdfs:subClassOf cinema:Person ;
                rdfs:label "Director" .


###  http://teochris.local/cinema#DirectorsOrWriters
cinema:DirectorsOrWriters rdf:type owl:Class ;
                          owl:equivalentClass [ rdf:type owl:Class ;
                                                owl:unionOf ( cinema:Actor
                                                              cinema:Writer
                                                            )
                                              ] ;
                          rdfs:subClassOf cinema:Person ;
                          rdfs:comment "Κλάση που περιέχει την ένωση των σκηνοθετών με τους συγγραφείς." .


###  http://teochris.local/cinema#Genre
cinema:Genre rdf:type owl:Class ;
             rdfs:label "Genre" .


###  http://teochris.local/cinema#Movie
cinema:Movie rdf:type owl:Class ;
             rdfs:label "Movie" .


###  http://teochris.local/cinema#MovieAward
cinema:MovieAward rdf:type owl:Class ;
                  rdfs:subClassOf cinema:Award ;
                  rdfs:comment "Award that is given out to a movie (for example Best Picture)" ;
                  rdfs:label "Movie award" .


###  http://teochris.local/cinema#MovieSeries
cinema:MovieSeries rdf:type owl:Class ;
                   rdfs:comment "A movie series." ;
                   rdfs:label "Movie series" .


###  http://teochris.local/cinema#Person
cinema:Person rdf:type owl:Class ;
              rdfs:label "Person" .


###  http://teochris.local/cinema#PersonAward
cinema:PersonAward rdf:type owl:Class ;
                   rdfs:subClassOf cinema:Award ;
                   rdfs:comment "Award that is given out to a person (for example Best Actor)" ;
                   rdfs:label "Person award" .


###  http://teochris.local/cinema#ProductionCompany
cinema:ProductionCompany rdf:type owl:Class ;
                         rdfs:subClassOf cinema:Company ;
                         rdfs:label "Production company" .


###  http://teochris.local/cinema#Writer
cinema:Writer rdf:type owl:Class ;
              rdfs:subClassOf cinema:Person ;
              rdfs:label "Writer" .


